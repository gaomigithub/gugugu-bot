.index {
  /* width: 100vh; */
  height: 100vh;
  margin: 0;
  padding: 0;
}

.container {
  width: 300rpx;
  /* height: 300px; */
  /* background: orange; */
  margin: 0 auto; /*水平居中*/
  position: relative;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-flex-direction: column;
      -ms-flex-direction: column;
          flex-direction: column;
  -webkit-align-items: center;
      -ms-flex-align: center;
          align-items: center;
  top: 30%; /*偏移*/
  -webkit-transform: translateY(-50%);
      -ms-transform: translateY(-50%);
          transform: translateY(-50%);
}

.container .center{
  margin: auto;
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
}

